{
    "name": "zgkaizen/sai",
    "description": "Smart Alarm IoT - A Raspberry Pi-based Smart Alarm system",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "google/apiclient": "^2.15",
        "phpmailer/phpmailer": "^6.9",
        "firebase/php-jwt": "^6.10",
        "vlucas/phpdotenv": "^5.6",
        "monolog/monolog": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "SAI\\": "backend/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 